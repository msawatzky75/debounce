{
	"name": "@msawatzky75/debounce",
	"packageManager": "yarn@3.2.1",
	"description": "Creates and returns a new debounced version of the passed function that will postpone its execution until after wait milliseconds have elapsed since the last time it was invoked",
	"version": "1.0.0",
	"repository": "git://github.com/msawatzky75/debounce",
	"main": "./dist/debounce.cjs.js",
	"module": "./dist/debounce.es.js",
	"exports": {
		".": {
			"require": "./dist/debounce.cjs.js",
			"import": "./dist/debounce.es.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"type": "module",
	"scripts": {
		"build": "vite build",
		"test": "vitest"
	},
	"license": "MIT",
	"keywords": [
		"function",
		"throttle",
		"invoke"
	],
	"devDependencies": {
		"@rollup/plugin-typescript": "^8.3.3",
		"@types/node": "^16.9.0",
		"@types/prettier": "^2",
		"prettier": "^2.6.2",
		"rollup": "^2.75.6",
		"rollup-plugin-typescript2": "^0.32.1",
		"tslib": "^2.4.0",
		"typescript": "^4.7.3",
		"vite": "^2.9.12",
		"vitest": "^0.14.2"
	}
}
